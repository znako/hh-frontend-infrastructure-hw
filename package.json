{
  "name": "hh-frontend-infrastructure",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "pretty": "prettier . --check",
    "pretty-fix": "prettier . --write",
    "ts-lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "ts-lint-fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "css-lint": "stylelint '**/*.css'",
    "preview": "vite preview",
    "prepare": "husky install",
    "deploy": "yarn build && gh-pages -d dist"
  },
  "homepage": "https://znako.github.io/hh-frontend-infrastructure-hw",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "13.13.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "@hh.ru/eslint-config": "15.1.1",
    "@hh.ru/eslint-plugin-import-rules": "1.5.2",
    "@hh.ru/stylelint-config-hh": "^10.2.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-dollar-sign": "1.0.2",
    "eslint-plugin-etc": "1.5.2",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.0",
    "prettier": "2.8.7",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-order": "~4.1.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "~3.19.0",
    "typescript": "4.9.5",
    "vite": "^5.0.0"
  },
  "resolutions": {
    "eslint": "7.5.0"
  }
}
